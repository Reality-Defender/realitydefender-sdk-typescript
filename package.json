{
  "name": "@realitydefender/realitydefender",
  "version": "0.1.2",
  "description": "SDK for the Reality Defender API for deepfake detection",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "README.md",
    "LICENSE"
  ],
  "packageManager": "yarn@4.6.0",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "tsc --declaration --declarationMap",
    "test": "jest --coverage --detectOpenHandles --forceExit",
    "prepare": "yarn build",
    "example": "ts-node examples/basic.ts",
    "lint": "tsc --noEmit",
    "clean": "rimraf dist",
    "setup": "yarn install && yarn build && cd examples && yarn install"
  },
  "keywords": [
    "deepfake",
    "detection",
    "ai",
    "sdk",
    "realitydefender"
  ],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.18",
    "jest": "^29.7.0",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "axios": "^1.9.0"
  }
}
