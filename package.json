{
  "name": "@realitydefender/realitydefender",
  "version": "0.1.8",
  "description": "SDK for the Reality Defender API for deepfake detection",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "!dist/tests",
    "README.md",
    "LICENSE"
  ],
  "packageManager": "yarn@4.6.0",
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": ">=20.0.0 <=25.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --declaration --declarationMap",
    "test": "jest --coverage --detectOpenHandles --forceExit",
    "prepare": "yarn build",
    "example": "ts-node examples/basic.ts",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "types": "tsc --noEmit --skipLibCheck",
    "clean": "rimraf dist",
    "setup": "yarn install && yarn build && cd examples && yarn install"
  },
  "keywords": [
    "deepfake",
    "detection",
    "ai",
    "sdk",
    "realitydefender"
  ],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.18",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.28.0",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  },
  "dependencies": {
    "axios": "^1.9.0"
  }
}
